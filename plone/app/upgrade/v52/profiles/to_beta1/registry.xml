<?xml version="1.0" encoding="utf-8"?>
<registry>

  <records interface="plone.base.interfaces.INavigationSchema"
           prefix="plone"
           purge="False"
  >
    <value key="navigation_depth">1</value>
  </records>

  <!-- changes from PLIP 1653 - restructure static resources -->
  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/expect"
           remove="True"
  />
  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/js-shortcuts"
           remove="True"
  />
  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/marked"
           remove="True"
  />
  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/rjs"
           remove="True"
  />
  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/react"
           remove="True"
  />
  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/JSXTransformer"
           remove="True"
  />
  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/sinon"
           remove="True"
  />

  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/plone-patterns-toolbar"
           purge="False"
  >
    <value key="js">++resource++mockup/toolbar/pattern.js</value>
    <value key="css">
      <element>++resource++mockup/toolbar/pattern.toolbar.less</element>
    </value>
  </records>

  <records interface="plone.base.interfaces.IResourceRegistry"
           prefix="plone.resources/thememapper"
           purge="False"
  >
    <value key="js">++plone++static/thememapper.js</value>
    <value key="css">
      <element>++plone++static/thememapper.less</element>
    </value>
  </records>

  <records interface="plone.base.interfaces.IBundleRegistry"
           prefix="plone.bundles/thememapper"
           purge="False"
  >
    <value key="resources">
      <element>thememapper</element>
    </value>
    <value key="enabled">False</value>
    <value key="jscompilation">++plone++static/thememapper-compiled.min.js</value>
    <value key="csscompilation">++plone++static/thememapper-compiled.css</value>
    <value key="last_compilation">2018-10-01 23:00:00</value>
  </records>

  <records interface="Products.ResourceRegistries.interfaces.settings.IResourceRegistriesSettings"
           prefix="plone"
           remove="True"
  />
</registry>
